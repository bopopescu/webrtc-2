defines = -DV8_DEPRECATION_WARNINGS -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DCR_CLANG_REVISION=239765-1 -DUSE_LIBJPEG_TURBO=1 $
    -DENABLE_CONFIGURATION_POLICY -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE $
    -DDONT_EMBED_BUILD_METADATA -DCLD_VERSION=2 -DDISABLE_FTP_SUPPORT=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA -DUSE_LIBPCI=1 -DUSE_OPENSSL=1 $
    -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1
includes = -Igen -I../../chromium/src/third_party/libyuv/include $
    -I../../chromium/src/third_party/libyuv
cflags = -isysroot $
    /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS8.4.sdk $
    -O0 -gdwarf-2 -fvisibility=hidden -Werror -Wnewline-eof $
    -miphoneos-version-min=7.0 -arch armv7 -Wall -Wendif-labels -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-selector-type-mismatch -Wheader-hygiene -Wno-char-subscripts $
    -Wno-unneeded-internal-declaration -Wno-covered-switch-default $
    -Wstring-conversion -Wno-c++11-narrowing -Wno-deprecated-register $
    -Wno-inconsistent-missing-override
cflags_c = -std=c99 -Xclang -load -Xclang $
    /Users/heberthbraga/Workspace/quimbik/webrtc/src/third_party/llvm-build/Release+Asserts/lib/libFindBadConstructs.dylib $
    -Xclang -add-plugin -Xclang find-bad-constructs -fcolor-diagnostics $
    -fstack-protector-all -Wno-undefined-bool-conversion $
    -Wno-tautological-undefined-compare
cflags_cc = -std=c++11 -stdlib=libc++ -fno-rtti -fno-exceptions $
    -fvisibility-inlines-hidden -fno-threadsafe-statics -Xclang -load $
    -Xclang $
    /Users/heberthbraga/Workspace/quimbik/webrtc/src/third_party/llvm-build/Release+Asserts/lib/libFindBadConstructs.dylib $
    -Xclang -add-plugin -Xclang find-bad-constructs -fcolor-diagnostics $
    -fstack-protector-all -Wno-undefined-bool-conversion $
    -Wno-tautological-undefined-compare
cflags_objc = $cflags_c -Wobjc-missing-property-synthesis
cflags_objcc = $cflags_cc -Wobjc-missing-property-synthesis $
    -fobjc-call-cxx-cdtors
arflags = 

build $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.compare_neon.o: $
    cxx ../../chromium/src/third_party/libyuv/source/compare_neon.cc
build $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.compare_neon64.o: $
    cxx ../../chromium/src/third_party/libyuv/source/compare_neon64.cc
build obj/chromium/src/third_party/libyuv/source/libyuv_neon.rotate_neon.o: $
    cxx ../../chromium/src/third_party/libyuv/source/rotate_neon.cc
build $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.rotate_neon64.o: $
    cxx ../../chromium/src/third_party/libyuv/source/rotate_neon64.cc
build obj/chromium/src/third_party/libyuv/source/libyuv_neon.row_neon.o: $
    cxx ../../chromium/src/third_party/libyuv/source/row_neon.cc
build obj/chromium/src/third_party/libyuv/source/libyuv_neon.row_neon64.o: $
    cxx ../../chromium/src/third_party/libyuv/source/row_neon64.cc
build obj/chromium/src/third_party/libyuv/source/libyuv_neon.scale_neon.o: $
    cxx ../../chromium/src/third_party/libyuv/source/scale_neon.cc
build $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.scale_neon64.o: $
    cxx ../../chromium/src/third_party/libyuv/source/scale_neon64.cc

build libyuv_neon.a: alink $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.compare_neon.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.compare_neon64.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.rotate_neon.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.rotate_neon64.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.row_neon.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.row_neon64.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.scale_neon.o $
    obj/chromium/src/third_party/libyuv/source/libyuv_neon.scale_neon64.o
