build obj/webrtc/video_engine/webrtc.vie_remb.o: cxx $
    ../../webrtc/video_engine/vie_remb.cc
build obj/webrtc/video_engine/webrtc.vie_sync_module.o: cxx $
    ../../webrtc/video_engine/vie_sync_module.cc

build libwebrtc.a: alink obj/webrtc/video/webrtc.audio_receive_stream.o $
