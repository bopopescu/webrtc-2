    obj/webrtc/video_engine/webrtc.call_stats.o $
    obj/webrtc/video_engine/webrtc.encoder_state_feedback.o $
    obj/webrtc/video_engine/webrtc.overuse_frame_detector.o $
    obj/webrtc/video_engine/webrtc.payload_router.o $
    obj/webrtc/video_engine/webrtc.report_block_stats.o $
    obj/webrtc/video_engine/webrtc.stream_synchronization.o $
    obj/webrtc/video_engine/webrtc.vie_capturer.o $
    obj/webrtc/video_engine/webrtc.vie_channel.o $
    obj/webrtc/video_engine/webrtc.vie_channel_group.o $
    obj/webrtc/video_engine/webrtc.vie_encoder.o $
    obj/webrtc/video_engine/webrtc.vie_receiver.o $
