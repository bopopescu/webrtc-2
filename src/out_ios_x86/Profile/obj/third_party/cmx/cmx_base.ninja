defines = -DV8_DEPRECATION_WARNINGS -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DCR_CLANG_REVISION=239765-1 -DUSE_LIBJPEG_TURBO=1 $
    -DENABLE_CONFIGURATION_POLICY -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE $
    -DDONT_EMBED_BUILD_METADATA -DCLD_VERSION=2 -DDISABLE_FTP_SUPPORT=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA -DUSE_LIBPCI=1 -DUSE_OPENSSL=1 -DNDEBUG $
    -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0 -DNS_BLOCK_ASSERTIONS=1
includes = -Igen -I../../third_party/cmx -I../..
cflags = -isysroot $
    /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator8.4.sdk $
    -Os -gdwarf-2 -fvisibility=hidden -Wnewline-eof $
    -mios-simulator-version-min=7.0 -arch i386 -Wendif-labels $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-selector-type-mismatch -Wheader-hygiene -Wno-char-subscripts $
    -Wno-unneeded-internal-declaration -Wno-covered-switch-default $
    -Wstring-conversion -Wno-c++11-narrowing -Wno-deprecated-register $
    -Wno-inconsistent-missing-override
cflags_c = -std=c99 -Xclang -load -Xclang $
    /Users/heberthbraga/Workspace/quimbik/webrtc/src/third_party/llvm-build/Release+Asserts/lib/libFindBadConstructs.dylib $
    -Xclang -add-plugin -Xclang find-bad-constructs -fcolor-diagnostics
cflags_cc = -std=c++11 -stdlib=libc++ -fno-rtti -fno-exceptions $
    -fvisibility-inlines-hidden -fno-threadsafe-statics -Xclang -load $
    -Xclang $
    /Users/heberthbraga/Workspace/quimbik/webrtc/src/third_party/llvm-build/Release+Asserts/lib/libFindBadConstructs.dylib $
    -Xclang -add-plugin -Xclang find-bad-constructs -fcolor-diagnostics
cflags_objc = $cflags_c
cflags_objcc = $cflags_cc -fobjc-call-cxx-cdtors
arflags = 

build obj/third_party/cmx/src/cmx_base.cmx_decoder.o: cxx $
    ../../third_party/cmx/src/cmx_decoder.cc
build obj/third_party/cmx/src/cmx_base.cmx_encoder.o: cxx $
    ../../third_party/cmx/src/cmx_encoder.cc
build obj/third_party/cmx/src/cmx_base.cmx_encoder_factory.o: cxx $
    ../../third_party/cmx/src/cmx_encoder_factory.cc
build obj/third_party/cmx/src/cmx_base.cmx_decoder_factory.o: cxx $
    ../../third_party/cmx/src/cmx_decoder_factory.cc

build libcmx_base.a: alink obj/third_party/cmx/src/cmx_base.cmx_decoder.o $
    obj/third_party/cmx/src/cmx_base.cmx_encoder.o $
    obj/third_party/cmx/src/cmx_base.cmx_encoder_factory.o $
    obj/third_party/cmx/src/cmx_base.cmx_decoder_factory.o
