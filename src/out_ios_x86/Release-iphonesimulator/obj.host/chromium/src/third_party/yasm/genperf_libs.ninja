ar = $ar_host
cc = $cc_host
cxx = $cxx_host
ld = $ld_host
ldxx = $ldxx_host
nm = $nm_host
readelf = $readelf_host
defines = -DV8_DEPRECATION_WARNINGS -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DCR_CLANG_REVISION=239765-1 -DUSE_LIBJPEG_TURBO=1 $
    -DENABLE_CONFIGURATION_POLICY -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE $
    -DDONT_EMBED_BUILD_METADATA -DCLD_VERSION=2 -DDISABLE_FTP_SUPPORT=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA -DHAVE_CONFIG_H -DUSE_LIBPCI=1 $
    -DUSE_OPENSSL=1 -DNDEBUG -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0 $
    -DNS_BLOCK_ASSERTIONS=1
includes = -Igen -I../../chromium/src/third_party/yasm/source/config/ios $
    -I../../chromium/src/third_party/yasm/source/patched-yasm
cflags = -isysroot $
    /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.9.sdk $
    -Os -gdwarf-2 -fvisibility=hidden -Wnewline-eof $
    -mmacosx-version-min=10.6 -arch x86_64 -Wendif-labels $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-selector-type-mismatch -Wheader-hygiene -Wno-char-subscripts $
    -Wno-unneeded-internal-declaration -Wno-covered-switch-default $
    -Wstring-conversion -Wno-c++11-narrowing -Wno-deprecated-register $
    -Wno-inconsistent-missing-override
cflags_c = -std=c99 -Xclang -load -Xclang $
    /Users/heberthbraga/Workspace/quimbik/webrtc/src/third_party/llvm-build/Release+Asserts/lib/libFindBadConstructs.dylib $
    -Xclang -add-plugin -Xclang find-bad-constructs -fcolor-diagnostics
cflags_cc = -std=c++11 -fno-rtti -fno-exceptions $
    -fvisibility-inlines-hidden -fno-threadsafe-statics -Xclang -load $
    -Xclang $
    /Users/heberthbraga/Workspace/quimbik/webrtc/src/third_party/llvm-build/Release+Asserts/lib/libFindBadConstructs.dylib $
    -Xclang -add-plugin -Xclang find-bad-constructs -fcolor-diagnostics
cflags_objc = $cflags_c
cflags_objcc = $cflags_cc -fobjc-call-cxx-cdtors
arflags = 

build $
    obj.host/chromium/src/third_party/yasm/source/patched-yasm/libyasm/genperf_libs.phash.o: $
    cc ../../chromium/src/third_party/yasm/source/patched-yasm/libyasm/phash.c
build $
    obj.host/chromium/src/third_party/yasm/source/patched-yasm/libyasm/genperf_libs.xmalloc.o: $
    cc $
    ../../chromium/src/third_party/yasm/source/patched-yasm/libyasm/xmalloc.c
build $
    obj.host/chromium/src/third_party/yasm/source/patched-yasm/libyasm/genperf_libs.xstrdup.o: $
    cc $
    ../../chromium/src/third_party/yasm/source/patched-yasm/libyasm/xstrdup.c

build obj.host/chromium/src/third_party/yasm/libgenperf_libs.a: alink $
    obj.host/chromium/src/third_party/yasm/source/patched-yasm/libyasm/genperf_libs.phash.o $
    obj.host/chromium/src/third_party/yasm/source/patched-yasm/libyasm/genperf_libs.xmalloc.o $
    obj.host/chromium/src/third_party/yasm/source/patched-yasm/libyasm/genperf_libs.xstrdup.o
